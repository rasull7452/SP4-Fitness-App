<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="697.0" prefWidth="380.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LoginController">
   <children>
      <PasswordField fx:id="password" layoutX="116.0" layoutY="336.0" promptText="Password" />
      <TextField fx:id="username" layoutX="116.0" layoutY="301.0" promptText="Username" />
      <Label layoutX="130.0" layoutY="206.0" text="Login">
         <font>
            <Font name="Calibri" size="56.0" />
         </font>
      </Label>
      <Button fx:id="loginButton" layoutX="164.0" layoutY="380.0" mnemonicParsing="false" onAction="#login" text="Login" />
   </children>
</AnchorPane>
